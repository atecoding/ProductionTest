	<!-- **************************************************** -->
	<device>
		Acoustic AIO
	  <ASIO_driver>ASIO Echo Acoustic AIO</ASIO_driver>

    <AIO_constant_current_test input="0" minimum="40000" maximum="50000" /> <!-- minimum and maximum are unsigned decimal 16-bit values -->
    <AIO_constant_current_test input="5" minimum="1000" maximum="60000" /> <!-- minimum and maximum are unsigned decimal 16-bit values -->


    <ignore>
      <AIO_set_mic_gain input="4" gain="10"/>
      <!-- gain values are 1, 10, or 100 -->

      <AIO_set_amp_gain output="2" gain="255"/>
      <!-- gain values are 26 or 255 -->

      <AIO_set_amp_gain output="3" gain="26"/>
      <!-- gain values are 26 or 255 -->

      <AIO_set_constant_current input="5" enabled="1"/>

      <AIO_TEDS_test input="4"/>
    <AIO_TEDS_test input="5"/>
    <AIO_TEDS_test input="6"/>
    <AIO_TEDS_test input="7"/>

      <AIO_write_test_adapter byte="0x55"/> <!-- value in hex -->

    </ignore>


    <ignore>
			<echo2_initial_power_test/>
		
			<echo2_touch_panel_test/>

