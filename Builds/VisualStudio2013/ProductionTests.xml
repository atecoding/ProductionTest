				<output_amplitude_db>-4.0</output_amplitude_db>
				<input>0</input>
				<output>2</output>
				<num_channels>2</num_channels>
				<sample_rate>48000</sample_rate>
				<min_input_db>-5.0</min_input_db>
				<max_input_db>-1.0</max_input_db>
				<wait_for_user>1</wait_for_user>
			</prompt>

