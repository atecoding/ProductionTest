			<set_touch_panel_reg address="INPUT2_SELECT" value="1"/>
			<set_touch_panel_reg address="INPUT1_LEVEL_POSITION" value="16"/>
			<set_touch_panel_reg address="INPUT2_LEVEL_POSITION" value="16"/>

