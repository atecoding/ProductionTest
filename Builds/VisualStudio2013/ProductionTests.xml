			<!-- Phantom power							 -->
			<!-- ======================================= -->
		
			<!-- Select mic input for both inputs -->
			<set_touch_panel_reg address="INPUT1_SELECT" value="0"/>
			<set_touch_panel_reg address="INPUT2_SELECT" value="0"/>
			<set_touch_panel_reg address="INPUT1_LEVEL_POSITION" value="0"/>
			<set_touch_panel_reg address="INPUT2_LEVEL_POSITION" value="0"/>
			<set_touch_panel_reg address="PHANTOM_POWER_SELECT" value="0"/>

			<message_box>Connect the phantom power tester to input 1 using the adapter cable.</message_box>

			<set_touch_panel_reg address="PHANTOM_POWER_SELECT" value="1"/>

			<message_box>
				Do you see three green lights on the phantom power tester?
				<show_yes_no>1</show_yes_no>
				<log>Phantom power input 1</log>
			</message_box>

			<set_touch_panel_reg address="PHANTOM_POWER_SELECT" value="0"/>

			<message_box>Connect the phantom power tester to input 2 using the adapter cable.</message_box>

			<set_touch_panel_reg address="PHANTOM_POWER_SELECT" value="1"/>

			<message_box>
				Do you see three green lights on the phantom power tester?
				<show_yes_no>1</show_yes_no>
