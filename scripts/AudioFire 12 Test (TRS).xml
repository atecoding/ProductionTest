	<!-- Echo AudioFire 12                                    -->
	<!-- **************************************************** -->
	<device>
		AudioFire12

	<ASIO_driver>AudioFire12</ASIO_driver>
    <CoreAudio_driver>AudioFire12</CoreAudio_driver>

    <!-- ================================== -->
    <!-- Analog Out 1/2 => Analog In 1/2 -->
    <!-- ================================== -->

    <prompt>
      	Analog Out 1/2 to Analog In 1/2
      	<output_amplitude_db>-10.0</output_amplitude_db>
      	<input>0</input>
      	<output>0</output>
      	<num_channels>2</num_channels>
      	<sample_rate>48000</sample_rate>
      	<min_input_db>-14.0</min_input_db>
      	<max_input_db>-9.0</max_input_db>
		<wait_for_user>1</wait_for_user>
    </prompt>

    <test>
      Analog Out 1/2 => Analog In 1/2
      <type>THD+N</type>
      	<input>0</input>
      	<output>0</output>
      	<num_channels>2</num_channels>
      	<sample_rate>48000</sample_rate>
      	<output_amplitude_db>-7.0</output_amplitude_db>
      	<pass_threshold_db>-90.0</pass_threshold_db>
    </test>
    <test>
      	<type>Dynamic range</type>
      	<input>0</input>
      	<output>0</output>
      	<num_channels>2</num_channels>
      	<sample_rate>48000</sample_rate>
      	<output_amplitude_db>-59.0</output_amplitude_db>
      	<pass_threshold_db>102.0</pass_threshold_db>
    </test>

    <test>
        <type>Frequency sweep response</type>
        <input>0</input>
        <output>0</output>
        <num_channels>2</num_channels>
        <sample_rate>48000</sample_rate>
        <output_amplitude_db>-10.0</output_amplitude_db>
        <output_frequency>20.0</output_frequency>
        <pass_threshold_db>1.0</pass_threshold_db>
        <sweep_time_seconds>2.0</sweep_time_seconds>
        <sweep_delay_seconds>1.0</sweep_delay_seconds>
        <sweep_fadein_seconds>0.1</sweep_fadein_seconds>
        <sweep_fadeout_seconds>0.1</sweep_fadeout_seconds>
        <sweep_record_seconds>4.0</sweep_record_seconds>
    </test>
	
    <!-- End of Testing Analog Out 1/2 -->

