<?xml version="1.0" encoding="utf-8" ?>
<root>
  
    <!-- **************************************************** -->
	<!-- Echo Acoustic AIO                                    -->
	<!-- **************************************************** -->
    
	<device>
    AIO-M1
    <Require_AIO_Test_Adapter>false</Require_AIO_Test_Adapter>
    <ASIO_driver>ASIO Echo Acoustic AIO</ASIO_driver>
    <CoreAudio_driver>EchoAIO-M1</CoreAudio_driver>

    <message_box>
	Is the blue LED lit?
	<show_yes_no>1</show_yes_no>
	<log>LED Test</log>
    </message_box>

    <AIO_firmware_version_test max_version="0x1000" />

    <AIO_module_type_test>
      <slot>0</slot>
      <!-- slot 0 == center slot -->
      <type>1</type>
      <!-- type 1 == TEDS microphone + class D amp analog module -->
    </AIO_module_type_test>

    <AIO_module_type_test>
      <slot>1</slot>
      <!-- slot 1 == outer slot -->
      <type>3</type>
      <!-- type 3 == MB module -->
    </AIO_module_type_test>

    <AIO_mikeybus>
      <read module="1" page="0x01" address="0x24" value="0xcc"/>

