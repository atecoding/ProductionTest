<?xml version="1.0" encoding="utf-8" ?>
<root>

  <!-- **************************************************** -->
  <!-- Echo AIO-S                                  -->
  <!-- **************************************************** -->
  
  <device USB_product_ID="0xa0">
      <ASIO_driver>ASIO Echo AIO</ASIO_driver>
      <CoreAudio_driver>EchoAIO</CoreAudio_driver>
      
      <AIO_firmware_version_test min_version="0x3800" />
      
      <AIO_module_type_test>
          <slot>0</slot>
          <!-- slot 0 == center slot -->
          <type>2</type>
          <!-- type 2 == AIO-S -->
      </AIO_module_type_test>
      
      <AIO_module_type_test>
          <slot>1</slot>
          <!-- slot 1 == outer slot -->
          <type>1</type>
          <!-- type 1 == AIO-A module -->
      </AIO_module_type_test>
      
      <AIO_calibration_verification_test/>

  </device>

</root>

