<?xml version="1.0" encoding="utf-8" ?>
<root>

  <!-- **************************************************** -->
  <!-- Echo AIO-M1                                          -->
  <!-- **************************************************** -->
  
  <device USB_product_ID="0xa1">
      <ASIO_driver>ASIO Echo AIO</ASIO_driver>
      <CoreAudio_driver>EchoAIO-M1</CoreAudio_driver>
      
      <AIO_firmware_version_test min_version="0x3800" />
      
      <AIO_module_type_test>
          <slot>0</slot>
          <!-- slot 0 == center slot -->
          <type>1</type>
          <!-- type 1 == AIO-A module -->
      </AIO_module_type_test>
      
      <AIO_module_type_test>
          <slot>1</slot>
          <!-- slot 1 == outer slot -->
          <type>3</type>
          <!-- type 3 == AIO-M module -->
      </AIO_module_type_test>
      
      <AIO_calibration_verification_test/>
  
  </device>

</root>

