<?xml version="1.0" encoding="utf-8" ?>
<root>

  <!-- **************************************************** -->
  <!-- Echo AIO-2                                           -->
  <!-- **************************************************** -->
  
  <device USB_product_ID="0xa0">
    <ASIO_driver>ASIO Echo AIO</ASIO_driver>
    <CoreAudio_driver>EchoAIO-M1</CoreAudio_driver>

    <AIO_firmware_version_test>
      <interface_module>
        <revision>0</revision>
        <!-- rev 1 interface module -->
        <min_version>0x1002</min_version>
        <max_version>0x1002</max_version>
      </interface_module>
      <interface_module>
        <revision>0x3000</revision>
        <!-- rev 2 interface module -->
        <min_version>0x3802</min_version>
      </interface_module>
    </AIO_firmware_version_test>

    <AIO_module_type_test>
        <slot>0</slot>  <!-- slot 1 == outer slot -->
        <type>1</type>  <!-- type 1 == TEDS microphone + class D amp analog module -->
    </AIO_module_type_test>
    
    <AIO_module_type_test>
        <slot>1</slot>  <!-- slot 1 == outer slot -->
        <type>1</type>  <!-- type 1 == TEDS microphone + class D amp analog module -->
    </AIO_module_type_test>
    
    <AIO_calibration_verification_test/>

  </device>

</root>
